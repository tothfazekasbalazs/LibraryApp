<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="405.0" prefWidth="524.0" stylesheets="@./css/book-form.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.banki.libraryapp.controller.SearchController">
   <children>
      <HBox prefHeight="38.0" prefWidth="615.0">
         <children>
            <Label alignment="CENTER" prefHeight="38.0" prefWidth="145.0" text="Kereső szöveg: " />
            <TextField fx:id="searchField" prefHeight="38.0" prefWidth="202.0" />
            <Button fx:id="seachButton" mnemonicParsing="false" prefHeight="38.0" prefWidth="110.0" text="Részletes kereső" />
            <Button fx:id="goBacktoStock" onAction="#goBack" mnemonicParsing="false" prefHeight="38.0" prefWidth="110.0" text="Vissza" />
         </children>
      </HBox>
      <VBox fx:id="dataContainer" layoutY="100.0" prefHeight="301.0" prefWidth="615.0" />
   </children>
</AnchorPane>
